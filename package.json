{
  "name": "lista-presentes",
  "description": "Lista de Presentes",
  "version": "0.0.1",
  "main": "app.js",
  "engines": {
    "node": "4.5.0"
  },
  "scripts": {
    "start": "node app.js",
    "sass": "node-sass --output-style compressed public/sass/acsset.scss > public/css/acsset.min.css && node-sass --output-style expanded public/sass/acsset.scss > public/css/acsset.css && node-sass --output-style expanded public/sass/style.scss > public/css/style.css",
    "acsset:build": "npm run sass"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "~4.14.0",
    "jsonwebtoken": "^7.2.1",
    "lomadee-api": "^0.1.6",
    "mongoose": "^4.6.8",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "pagseguro.js": "^0.2.0",
    "request": "^2.79.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "node-sass": "^3.10.0"
  },
  "author": "Matheus Piscioneri <mxczpiscioneri@gmail.com> (http://matheuspiscioneri.com.br)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mxczpiscioneri/lista-presentes.git"
  }
}
